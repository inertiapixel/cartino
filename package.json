{
  "name": "@inertiapixel/cartino",
  "version": "2.0.2",
  "description": "Cart management library for Node.js and TypeScript.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "cartino-init": "./dist/cli/cartino-init.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build": "tsc && chmod +x ./dist/cli/cartino-init.js",
    "postbuild": "npm run generate-types",
    "generate-types": "tsc --emitDeclarationOnly",
    "dev": "tsc-watch --onSuccess \"chmod +x ./dist/cli/cartino-init.js\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --quiet",
    "lint:fix": "npm run lint -- --fix",
    "test": "echo \"Add unit tests here\"",
    "prepublishOnly": "npm run check",
    "check": "npm run lint && npm run build && npm test"
  },
  "keywords": [
    "cart",
    "wishlist",
    "cart wishlist save for later node library",
    "ecommerce",
    "mern",
    "cart system",
    "express cart",
    "nodejs cart",
    "checkout system",
    "stateless cart",
    "opensource nodejs cart library"
  ],
  "author": {
    "name": "Md Asif",
    "email": "mdasiff007@gmail.com",
    "url": "https://github.com/inertiapixel"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^22.18.8",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^9.29.0",
    "globals": "^16.4.0",
    "inquirer": "^12.9.6",
    "mongoose": "^8.16.4",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "express": "^5.1.0"
  },
  "peerDependencies": {
    "express": "^5.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inertiapixel/cartino.git"
  },
  "homepage": "https://github.com/inertiapixel/cartino#readme",
  "bugs": {
    "url": "https://github.com/inertiapixel/cartino/issues"
  }
}
